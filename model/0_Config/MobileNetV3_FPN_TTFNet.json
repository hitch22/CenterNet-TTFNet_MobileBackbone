{
    "model_config":
    {
        "target_size": 320,

        "backbone":
        {
            "name":"MobileNetV3",
            "width_multiplier": 1.0,
            "modelSize":"large",
            "regularization":0.0,
            "isFreeze":false
        },
        "neck":
        {   "name":"FPN",
            "aggregation":"add",
            "isLite": true,
            "filters": [64, 32, 24, 24],
            "regularization":0.0,
            "isFreeze":false
        },
        "head":
        {
            "name": "TTFNet",
            "isLite": true,
            "filters": 256,
            "regularization":0.0,
            "isFreeze":false
        } 
    },

    "training_config":
    {
        "HeatLoss": 
        {
            "LossFunction": "Focal",
            "Alpha" : 2.0,
            "Gamma" : 4.0,
            "Weight" : 1.0
        },

        "BoxLoss": 
        {
            "LossFunction": "diou",
            "Weight" : 5.0
        },
        "batch_size": 64,
        "num_classes": 80,
        "epochs": 300,
        "initial_learning_rate": 5e-2
    }
}
